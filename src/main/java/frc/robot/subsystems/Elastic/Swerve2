package frc.robot.subsystems.Elastic;

import static edu.wpi.first.units.Units.*;

import edu.wpi.first.units.measure.Angle;
import edu.wpi.first.util.sendable.SendableBuilder;
import edu.wpi.first.wpilibj2.command.SubsystemBase;
import edu.wpi.first.wpilibj.Encoder;
import edu.wpi.first.wpilibj.smartdashboard.SmartDashboard;
import edu.wpi.first.util.sendable.Sendable;
import frc.robot.generated.TunerConstants;



public class Swerve2 extends SubsystemBase {




  public Swerve2(){


    SmartDashboard.putData("SwerveTestThingy", (Sendable) TunerConstants.FrontLeft);

    SmartDashboard.putNumber("test", 1);


      SmartDashboard.putData("Swerve Drive", new Sendable() {
 
      @Override      
        public void initSendable(SendableBuilder builder) {
          builder.setSmartDashboardType("SwerveDrive");
      
          builder.addDoubleProperty("Front Left Angle", () -> TunerConstants.FLencoderAngle().in(Rotations) * 2 * Math.PI, null);
          builder.addDoubleProperty("Front Left Velocity", () -> TunerConstants.FLencoderVelocity().in(RotationsPerSecond), null);
      
          builder.addDoubleProperty("Front Right Angle", () ->  TunerConstants.FRencoderAngle().in(Rotations) * 2 * Math.PI, null);
          builder.addDoubleProperty("Front Right Velocity", ()  -> TunerConstants.FRencoderVelocity().in(RotationsPerSecond), null);
      
          builder.addDoubleProperty("Back Left Angle", () -> TunerConstants.BLencoderAngle().in(Rotations) * 2 * Math.PI, null);
          builder.addDoubleProperty("Back Left Velocity", () -> TunerConstants.BLencoderVelocity().in(RotationsPerSecond), null);
      
          builder.addDoubleProperty("Back Right Angle", () -> TunerConstants.BLencoderAngle().in(Rotations) * 2 * Math.PI, null);
          builder.addDoubleProperty("Back Right Velocity", () -> TunerConstants.BLencoderVelocity().in(RotationsPerSecond), null);
      
          builder.addDoubleProperty("Robot Angle", () -> 1.0 /*  getRotation().getRadians() */, null);
        } 
      });
  
    }

}


